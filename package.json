{
  "name": "mdn-local",
  "version": "0.7.1",
  "description": "create offline mozilla developer network documents",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": ">=18.17.0"
  },
  "dependencies": {
    "agentkeepalive": "^4.6.0",
    "fast-glob": "^3.3.3",
    "json5": "^2.2.3",
    "mkdirp": "^3.0.1",
    "tough-cookie": "^6.0.0",
    "urijs": "^1.19.11",
    "website-scrap-engine": "^0.8.4"
  },
  "devDependencies": {
    "@jest/globals": "^30.1.2",
    "@types/node": "^24.10.0",
    "@types/tough-cookie": "^4.0.5",
    "@types/urijs": "^1.19.26",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.39.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.4",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "undici": "npm:@favware/skip-dependency@1.2.2"
  },
  "scripts": {
    "test": "npm run lint && node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint --fix src test",
    "clean": "node -e \"require('fs').rmSync('lib',{force:true,recursive:true})\"",
    "tsc": "tsc",
    "build": "npm run lint && npm run tsc && npm run copy",
    "copy": "node copy-src.js",
    "prepack": "npm run clean && npm run build",
    "postshrinkwrap": "node package-lock-resolved.js",
    "postinstall": "node -e \"require('fs').copyFileSync('fake-undici.d.ts', 'node_modules/cheerio/node_modules/undici/index.d.ts')\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/website-local/mdn-local.git"
  },
  "keywords": [
    "mdn"
  ],
  "author": "myfreeer",
  "license": "MPL-2.0"
}
